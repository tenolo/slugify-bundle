{
    "name": "tenolo/slugify-bundle",
    "description": "Converts specific data from entities into a slug.",
    "keywords": [
        "slugify",
        "symfony",
        "bundle",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/console": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/form": "~2.8|~3.0|~4.0",
        "symfony/validator": "~2.8|~3.0|~4.0",
        "symfony/options-resolver": "~2.8|~3.0|~4.0",

        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.6",

        "mmoreram/symfony-bundle-dependencies": "~1.0",
        "cocur/slugify": "~2.4|~3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/repo/private/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\SlugifyBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
