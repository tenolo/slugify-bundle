{
    "name": "tenolo/slugify-bundle",
    "description": ".",
    "keywords": [
        "slugify",
        "symfony",
        "bundle",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/console": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",

        "doctrine/orm": "~2.5",

        "tenolo/utilities": "~1.4",

        "cocur/slugify": "~2.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/repo/private/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\SlugifyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\SlugifyBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.1": "1.1-dev",
            "dev-release/1.0": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
